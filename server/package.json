{
  "name": "sunny-brackets-server",
  "version": "1.0.0",
  "description": "Backend server for It's Always Sunny in Philadelphia bracket and draft application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
